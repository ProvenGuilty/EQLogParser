                            <RepeatButton x:Name="HorizontalSmallIncrease"
                                          Visibility="Collapsed"
                                          Grid.Column="6"
                                          MinWidth="16"
                                          IsTabStop="False"
                                          Interval="50"
                                          Margin="1" />
                        </Grid>

