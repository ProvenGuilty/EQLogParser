                                          IsTabStop="False"
                                          Opacity="0" />
                            <RepeatButton x:Name="VerticalSmallIncrease"
                                          Grid.Row="0"
                                          Visibility="Collapsed"
                                          MinHeight="16"
                                          IsTabStop="False"
                                          Interval="50"
                                          Margin="1" />
